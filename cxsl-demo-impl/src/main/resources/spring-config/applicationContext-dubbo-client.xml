<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
	
	<dubbo:registry id="ec_fc_registry" address="${dubbo.registry.address2}"/>
	<dubbo:consumer check="false" /> 
  		
	<dubbo:reference id="dataExporterService" retries="0" 
                     interface="com.midea.dataexporter.api.service.DataExporterService"
                     group="${exporter_service_group}"
                     check="false"
                     version="0.0.1"/>
                     
    <dubbo:reference id="tradeFacade" 
			interface="com.midea.trade.gateway.api.service.TradeFacade" />
			
	<dubbo:reference id="pkGeneratorFacade" 
			interface="com.midea.trade.core.pk.service.facade.PkGeneratorFacade" />
			
    <dubbo:reference id="refundApplyFacade" 
			interface="com.midea.trade.core.api.service.RefundApplyFacade" />
			
	<dubbo:reference id="openInvoiceMainFacade" 
			interface="com.midea.ec.fc.api.OpenInvoiceMainFacade" 
			registry="ec_fc_registry"
			check="false"/>
			
	<dubbo:reference id="fcCustomerFacade" 
			interface="com.midea.ec.fc.api.FcCustomerFacade" 
			registry="ec_fc_registry"
			check="false"/>
	
	<dubbo:reference id="fcCustomerConfigFacade" 
			interface="com.midea.ec.fc.api.FcCustomerConfigFacade" 
			registry="ec_fc_registry"
			check="false"/>
			
	<dubbo:reference id="pgExporterTaskFacade" 
			interface="com.midea.pgexporter.api.facade.PgExporterTaskFacade" />
    <!-- 查询返现活动金额 -->
    <dubbo:reference interface="com.midea.ec.fc.api.QueryCashbackFacade" id="queryCashbackFacade" registry="ec_fc_registry"/>
</beans>